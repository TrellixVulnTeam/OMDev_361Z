## --*- Makefile -*--
## Make listed targets even in case a file with same name exists.
.PHONY: \
 this \
 all clean install test distclean \
 this-all this-clean this-install this-test this-distclean \
 all-this clean-this install-this test-this distclean-this \
 $(eof)

## delete suffix rules - shortens output when using -d and is not
## used anyway (and should not be used).
.SUFFIXES:

Makefile :: /c/dev/antlr-2.7.7/scripts/../$(subdir)/Makefile.in  \
            /c/dev/antlr-2.7.7/scripts/config.deps \
					  /c/dev/antlr-2.7.7/scripts/config.make \
					  /c/dev/antlr-2.7.7/scripts/config.vars \
            /c/dev/antlr-2.7.7/config.status
	@echo "*** update $(subdir)/Makefile"
	@cd /c/dev/antlr-2.7.7 && CONFIG_FILES="$(subdir)/Makefile" /bin/sh ./config.status -q

/c/dev/antlr-2.7.7/scripts/config.deps : \
	/c/dev/antlr-2.7.7/scripts/../scripts/config.deps.in
	@echo "*** update $@"
	@cd /c/dev/antlr-2.7.7 && CONFIG_FILES=scripts/config.deps /bin/sh ./config.status -q

/c/dev/antlr-2.7.7/scripts/config.make : \
	/c/dev/antlr-2.7.7/scripts/../scripts/config.make.in
	@echo "*** update $@"
	@cd /c/dev/antlr-2.7.7 && CONFIG_FILES=scripts/config.make /bin/sh ./config.status -q

/c/dev/antlr-2.7.7/scripts/config.vars : \
	/c/dev/antlr-2.7.7/scripts/../scripts/config.vars.in
	@echo "*** update $@"
	@cd /c/dev/antlr-2.7.7 && CONFIG_FILES=scripts/config.vars /bin/sh ./config.status -q

/c/dev/antlr-2.7.7/config.status : \
	/c/dev/antlr-2.7.7/scripts/../configure
	@echo "*** reconfigure $@ - stay tuned .."
	@cd /c/dev/antlr-2.7.7 && /bin/sh ./config.status -q --recheck
	@echo "*** update all configured files .."
	@cd /c/dev/antlr-2.7.7 && /bin/sh ./config.status -q

### In a pure devel mode there should be also a dependency listed on how
### to make configure out of  configure.in.  This  requires that usr has 
### m4  and  autoconf (proper version) installed. Appropriate checks are 
### not done in configure. If so, then uncomment next lines:
###
###   @rule_configure_configure_in@
###

