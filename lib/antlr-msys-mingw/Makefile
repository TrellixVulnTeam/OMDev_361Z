## GNU make - how to make object file
/c/dev/antlr-2.7.7/lib/cpp/src/%.o : /c/dev/antlr-2.7.7/lib/cpp/src/../../../lib/cpp/src/%.cpp
	@ /bin/sh /c/dev/antlr-2.7.7/scripts/cxx.sh $<

## Build static library. How the library is build as well as
## libraries's name is hidden by our script, ie. configured.
/c/dev/antlr-2.7.7/lib/cpp/src/libantlr.a :: $(antlr_obj_FILES)
	@ /bin/sh /c/dev/antlr-2.7.7/scripts/lib.sh $(antlr_obj_FILES)

clean:
	/bin/rm.exe -r -f *.obj *.o *.a  *.lib *.so *.dll *~ /c/dev/antlr-2.7.7/lib/cpp/src/libantlr.a

## use this target if you just want to rebuild the lib without
## compiling again.
clean-lib:
	/bin/rm.exe -r -f /c/dev/antlr-2.7.7/lib/cpp/src/libantlr.a

distclean: clean
	/bin/rm.exe -r -f Makefile

test:

install: this-install

antlr_lib_FILES = \
  /c/dev/antlr-2.7.7/lib/cpp/src/libantlr.a \
  $(eol)

this-install: /c/dev/antlr-2.7.7/lib/cpp/src/libantlr.a 
	@$(MKDIR) -p "$(libdir)"
	@/bin/echo "install C++ core files .. "
	@for f in $(antlr_lib_FILES) ; do \
		/bin/echo "install $${f}" ; \
		if test -f "$${f}" ; then \
			$(INSTALL) -m 444 "$${f}" "$(libdir)" ; \
		fi ;\
	done


.PHONY: all clean distclean compile lib install test clean-lib this-install

## dependencies
$(antlr_obj_FILES) : /c/dev/antlr-2.7.7/scripts/cxx.sh
/c/dev/antlr-2.7.7/lib/cpp/src/libantlr.a        :: /c/dev/antlr-2.7.7/scripts/lib.sh

## other dependencies to be listed below
